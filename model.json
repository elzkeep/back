{
  "buildtool": "1.1",
  "database": "aoi_develop",
  "user": "aoi",
  "server": "aoi.netb.co.kr",
  "password": "aoidb",
  "auth": "jwt",
  "adminLevel": "3",  
  "table": [
    {
      "name": "user",
      "map": [
        {
          "name": "level",
          "data": ["", "normal:Normal", "premium:Manager", "admin:Administrator"]
        },
        {
          "name": "status",
          "data": ["", "use:사용", "notuser:사용안함"]
        }
      ],  
      "method": [
        "GetByEmail",
        "CountByEmail",
        "FindByLevel",        
        "UpdateImageById"
      ]
    },    
    {
      "name": "token",             
      "method" : [
        "GetByUser"
      ],
      "session" : [
        {
          "key": "id",
          "column": "user"
        }
      ]  
    },
    {
      "name": "game",             
      "map": [        
        {
          "name": "status",
          "data": ["", "ready:대기중", "faction:종족선택중", "normal:게임중", "end:게임종료"]
        },
        {
          "name": "illusionists",
          "data": ["", "normal:너프 없음", "vp2:2, 3점으로 너프", "vp1:1, 2점으로 너프", "vp0:점수안줌", "ban:사용 안함"]
        }
      ]            
    },
    {
      "name": "gamelist",             
      "map": [        
        {
          "name": "status",
          "data": ["", "ready:대기중", "faction:종족선택중", "normal:게임중", "end:게임종료"]
        },
        {
          "name": "illusionists",
          "data": ["", "normal:너프 없음", "vp2:2, 3점으로 너프", "vp1:1, 2점으로 너프", "vp0:점수안줌", "ban:사용 안함"]
        }
      ]            
    },
    {
      "name": "smsauth",             
      "method" : [
        "GetByHpNumber",
        "DeleteByHp"
      ]      
    },
    {
      "name": "gameuser",
      "join": [
        {
          "name": "user",
          "column": "user"
        }
      ],
      "method" : [
        "FindByGame",
        "CountByGame",
        "GetByGameUser",
        "CountByGameUser",
        "DeleteByGame"
      ]      
    },
    {
      "name": "gamehistory",      
      "method" : [
        "FindByGame",
        "CountByGame",        
        "DeleteByGame"
      ]      
    },
    {
      "name": "gametile",      
      "method" : [
        "FindByGame",
        "CountByGame",        
        "DeleteByGame"
      ]      
    },
    {
      "name": "gameundo",
      "map": [
        {
          "name": "status",
          "data": ["", "wait:Wait", "complete:Complete"]
        }
      ],
      "method" : [
        "FindByGame",
        "CountByGame",
        "DeleteByGame"
      ]
    },
    {
      "name": "gameundoitem",
      "map": [
        {
          "name": "status",
          "data": ["", "accept:Accept", "reject:Reject"]
        }        
      ],
      "method" : [
        "FindByGame",
        "CountByGame",
        "DeleteByGame"
      ]
    }
  ]   
}
